  "version": "0.2.0",
  "configurations": [
    {
      "disableNetworkCache": true,
      "name": "chrome:attach:3001",
      "port": 9222,
      "request": "attach",
      "showAsyncStacks": true,
      "skipFiles": ["node_modules", "./ct-desktop/node_modules"],
      "smartStep": true,
      "type": "chrome",
      "urlFilter": "http://localhost:3001/*",
      "webRoot": "${workspaceFolder}/ct-desktop/build/"
    },
    {
      "disableNetworkCache": true,
      "name": "chrome:attach:3002",
      "port": 9222,
      "request": "attach",
      "showAsyncStacks": true,
      "skipFiles": ["node_modules", "./ct-mobile/node_modules"],
      "smartStep": true,
      "type": "chrome",
      "urlFilter": "http://localhost:3002/*",
      "webRoot": "${workspaceFolder}/ct-mobile/build/"
    }
  ]
}
